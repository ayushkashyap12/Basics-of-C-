 #include <stdio.h>


int main()

{ int i,first=0,second=1,third,n;
 printf("enter the series till you want to print");
 scanf("%d",&n);
 
 printf("%d %d",first,second);

   for(i=3;i<=n;i++)
   {
       third=first+second;
       first=second;
       second=third;
       
       printf(" %d ",third);
       
   } //printf(" %d %d %d",first,second,third);
  
}
